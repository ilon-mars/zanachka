{
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "e2e"
  ],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "es2020",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "lib": [
      "ES2022",
      "DOM"
    ],
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    },
  ]
}
